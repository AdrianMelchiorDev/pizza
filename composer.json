{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.2.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "beberlei/doctrineextensions": "^1.2",
    "dompdf/dompdf": "^0.8.3",
    "knplabs/knp-menu-bundle": "^3.0",
    "phpoffice/phpspreadsheet": "^1.9",
    "ramsey/uuid-doctrine": "^1.5",
    "sensio/framework-extra-bundle": "^5.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^5.0",
    "symfony/console": "^5.0",
    "symfony/dotenv": "^5.0",
    "symfony/expression-language": "^5.0",
    "symfony/flex": "^1.1",
    "symfony/form": "^5.0",
    "symfony/framework-bundle": "^5.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "^5.0",
    "symfony/security-bundle": "^5.0",
    "symfony/serializer-pack": "*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^5.0",
    "symfony/twig-bundle": "^5.0",
    "symfony/validator": "^5.0",
    "symfony/web-link": "^5.0",
    "symfony/webpack-encore-bundle": "^1.3",
    "symfony/yaml": "^5.0",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "slevomat/coding-standard": "^5.0",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/debug-pack": "*",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "cs-check": "phpcs -s -n --runtime-set ignore_warnings_on_exit true",
    "cs-fix": "phpcbf",
    "rebuild-testdb": "php bin/console doctrine:schema:drop --full-database --env=test --force && php bin/console doctrine:migrations:migrate --env=test -n && php bin/console doctrine:fixtures:load --env=test --append",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}
